}
.fs_11 {
    font-size: 1.1rem;
}
.fs_12 {
    font-size: 1.2rem;
}
.fs_13 {
    font-size: 1.3rem;
}
.fs_14 {
    font-size: 1.4rem;
}
.fs_15 {
    font-size: 1.5rem;
}
.fs_16 {
