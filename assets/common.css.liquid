}

/* letter-spacing */
.ls_min01 {
    letter-spacing: -.01em;
}
.ls_min02 {
    letter-spacing: -.02em;
}
.ls_min03 {
    letter-spacing: -.03em;
}
.ls_min04 {
    letter-spacing: -.04em;
}
